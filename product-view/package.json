    "build:prod": "npm run build:externals && npm run copy:assets && npm run npx-build-plus:copy-assets",
    "build:externals": "ng build --extra-webpack-config webpack.externals.js --prod --single-bundle --output-hashing none",
    "serve": "http-server --gzip",
